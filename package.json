{
  "name": "template",
  "version": "1.0.0",
  "description": "Template for cypress projects. Has all the needed basic tools",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "cypress:run": "npm run prereport && cypress run; npm run junit:merge; npm run mochawesome:merge",
    "delete:reports": "rm -r cypress/results || true && rm -r mochawesome-report || true",
    "prereport": "npm run delete:reports",
    "report": "cypress run --reporter cypress-multi-reporters --reporter-options configFile=reporter-config.json",
    "mochawesome:merge": "npx mochawesome-merge 'cypress/results/mochawesome/*.json' > mochawesome.json && npx marge mochawesome.json",
    "junit:merge": "npx junit-merge -d cypress/results/junit -o cypress/results/junit/result.xml",
    "dockerbuild:cypress": "docker build -t cypress .",
    "dockercompose:cypress": "docker compose up --remove-orphans"
  },
  "keywords": [
    "Cypress",
    "Javascript",
    "Typescript"
  ],
  "author": "Phillip Onfri",
  "license": "ISC",
  "devDependencies": {
    "@cypress/grep": "^4.1.0",
    "@cypress/skip-test": "^2.6.1",
    "@types/cypress": "^1.1.3",
    "@types/node": "^20.14.10",
    "bootstrap": "^5.3.3",
    "cypress": "^13.13.0",
    "cypress-iframe": "^1.0.1",
    "cypress-localstorage-commands": "^2.2.6",
    "cypress-multi-reporters": "^1.6.4",
    "cypress-wait-until": "^3.0.1",
    "junit-merge": "^2.0.0",
    "mocha-junit-reporter": "^2.2.1",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.3.0",
    "mochawesome-report-generator": "^6.2.0",
    "normalize.css": "^8.0.1",
    "npm-run-all": "^4.1.5",
    "start-server-and-test": "^2.0.4",
    "typescript": "^5.5.3",
    "webpack": "^5.92.1",
    "webpack-bundle-analyzer": "^4.10.2"
  }
}
