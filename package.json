{
  "name": "template",
  "version": "1.0.0",
  "description": "Template for cypress projects. Has all the needed basic tools",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "cypress:run": "npm run prereport && cypress run --reporter cypress-multi-reporters --reporter-options configFile=reporter-config.json && npm run junit:merge && npm run mochawesome:merge && npm run allure:generate",
    "cypress:run_spec": "npx cypress run --spec './cypress/e2e/visit.site.spec.ts' --record --key 07597a08-0895-43e5-a9a4-1fce7923f75d",
    "cypress:run_browsers": "npm run cypress:run_spec -- --browser chrome --group Chrome --ci-build-id $ID & npm run cypress:run_spec -- --browser electron --group Electron --ci-build-id $ID",
    "cypress:run_GenID": "ID=$(uuid) npm run cypress:run_spec",
    "delete:reports": "rm -r cypress/results || true && rm -r mochawesome-report || true",
    "prereport": "npm run delete:reports",
    "report": "cypress run --reporter cypress-multi-reporters --reporter-options configFile=reporter-config.json",
    "mochawesome:merge": "npx mochawesome-merge 'cypress/results/mochawesome/*.json' > mochawesome.json && npx marge mochawesome.json",
    "junit:merge": "npx junit-merge -d cypress/results/junit -o cypress/results/junit/result.xml",
    "run:report": "live-server dockerReports --port=8080 --open=mochawesome.html",
    "dockerize": "docker build -t cypress_temp .",
    "docker:compose": "docker-compose run --rm --remove-orphans cypress_template",
    "docker:run": "docker run --rm --name cypress_template cypress_temp",
    "docker:start": "docker start cypress_template",
    "allure:open": "npx allure open ./allure-results -p 3000",
    "allure:cleanup": "rm -rf allure-report allure-results || true",
    "allure:run-open": "npm run cypress:run && npm run allure:open",
    "allure:generate": "npx allure generate -c",
    "kill:port": "lsof -ti :8080 | xargs kill -9"
  },
  "keywords": [
    "Cypress",
    "Javascript",
    "Typescript"
  ],
  "author": "Phillip Onfri",
  "license": "ISC",
  "devDependencies": {
    "@cypress/grep": "^4.1.1",
    "@cypress/skip-test": "^2.6.1",
    "@types/cypress": "^1.1.6",
    "allure-commandline": "^2.34.1",
    "allure-cypress": "^3.3.3",
    "bootstrap": "^5.3.7",
    "cypress": "^14.5.4",
    "cypress-axe": "^1.6.0",
    "cypress-iframe": "^1.0.1",
    "cypress-localstorage-commands": "^2.2.8",
    "cypress-multi-reporters": "^2.0.5",
    "cypress-wait-until": "^3.0.2",
    "dotenv": "^17.2.1",
    "junit-merge": "^2.0.0",
    "kill-port": "^2.0.1",
    "live-server": "^1.2.2",
    "mocha-junit-reporter": "^2.2.1",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^5.0.0",
    "mochawesome-report-generator": "^6.2.0",
    "normalize.css": "^8.0.1",
    "npm-run-all": "^4.1.5",
    "playwright-webkit": "^1.54.2",
    "start-server-and-test": "^2.0.13",
    "typescript": "^5.9.2",
    "webpack": "^5.101.2",
    "webpack-bundle-analyzer": "^4.10.2"
  }
}
